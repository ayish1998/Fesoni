# Docker Environment Configuration for Fesoni

# Kong API Gateway
VITE_KONG_GATEWAY_URL=http://localhost:8000
VITE_KONG_API_KEY=your-kong-api-key-here

# LavinMQ Message Queue
VITE_LAVINMQ_URL=http://localhost:15672
VITE_LAVINMQ_USER=fesoni
VITE_LAVINMQ_PASS=fesonipass

# API Keys (Replace with your actual keys)
VITE_OPENAI_API_KEY=your-openai-api-key-here
VITE_RAPIDAPI_KEY=your-rapidapi-key-here
VITE_FOXIT_API_KEY=your-foxit-api-key-here
VITE_FOXIT_BASE_URL=http://localhost:3003

# Redis Cache
VITE_REDIS_URL=redis://localhost:6379

# Mock API URLs (for development)
VITE_MOCK_OPENAI_URL=http://localhost:3001
VITE_MOCK_AMAZON_URL=http://localhost:3002
VITE_MOCK_FOXIT_URL=http://localhost:3003

# Development Mode
VITE_DEV_MODE=true